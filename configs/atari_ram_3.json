{
  "env": [
    {
      "name": ["Enduro-ram-v4", "Pong-ram-v4", "Qbert-ram-v4", "SpaceInvaders-ram-v4"],
      "train_steps": [5e7],
      "test_per_episodes": [10],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {
      "name": ["DQN"]
    }
  ],
  "device": ["cpu"],
  "hidden_layers": [[128,128,128], [128,128,128,128]],
  "memory_type": ["Replay"],
  "memory_size": [5e5],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [5e4],
  "epsilon_steps": [1e6],
  "epsilon_start": [1.0],
  "epsilon_end": [0.01],
  "epsilon_decay": [0.999],
  "loss": ["MSELoss"],
  "optimizer": ["RMSprop"],
  "lr": [3e-3, 1e-3, 3e-4, 1e-4, 3e-5],
  "batch_size": [32],
  "display_interval": [10],
  "rolling_score_window": [1000],
  "discount": [0.99],
  "seed": [1],
  "show_tb": [true],
  "gradient_clip": [5],
  "target_network_update_freqency": [10000],
  "sgd_update_frequency": [4],
  "generate_random_seed": [true]
}