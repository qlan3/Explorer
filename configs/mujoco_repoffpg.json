{
  "env": [
    {
      "name": ["HalfCheetah-v2", "Hopper-v2", "Walker2d-v2", "Swimmer-v2", "Ant-v2", "Reacher-v2"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "train_steps": [3e6], 
  "test_per_episodes": [50],
  "exploration_steps": [1e4],
  "memory_size": [1e6],
  "agent": [{"name": ["RepOffPG"]}],
  "optimizer": [
    {
      "name": ["Adam"],
      "actor_kwargs": [{"lr": [3e-4]}],
      "critic_kwargs": [{"lr": [1e-3]}]
    }
  ],
  "batch_size": [64],
  "network_update_frequency": [1],
  "discount": [0.99],
  "polyak": [0.995],
  "gradient_clip": [-1],
  "hidden_layers": [[256,256]],
  "hidden_act": ["ReLU"],
  "display_interval": [20],
  "rolling_score_window": [{"Train": [20], "Test": [10]}],
  "seed": [1],
  "device": ["cpu"],
  "show_tb": [true],
  "generate_random_seed": [true]
}